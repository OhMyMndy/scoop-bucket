    "hash": "673cb559bc60f3d1e45b4cca22b3092f49649875ae983f2a58e38da4e6f3dfce",
    "pre_install": [
        "    # Rename",
        "Get-ChildItem -r \"$dir\\阿里云盘小白羊版*.exe\" | Rename-Item -newname \"$dir\\阿里云盘小白羊版.exe\"",
        "    # Create files for persisting",
        "function PersistsFile([String] $file) {",
        "    if (!(Test-Path \"$persist_dir\\$file\")) {",
        "        New-Item \"$dir\\$file\" -Type File | Out-Null",
        "    }",
        "}",
        "@('data/user.db','data/debug.log') | ForEach-Object { PersistsFile $_ }"
    ],
    "extract_dir": "阿里云盘小白羊版",
    "shortcuts": [
        [
            "阿里云盘小白羊版.exe",
            "阿里云盘小白羊版"
        ]
    ],
    "persist": [
        "data/user.db",
        "data/debug.log",
        "data/aria2.conf"
    ],
    "checkver": {
        "github": "https://github.com/liupan1890/aliyunpan"
    },
    "autoupdate": {
        "url": "https://github.com/liupan1890/aliyunpan/releases/download/v$version/Win.v$version.7z"
    }
}
