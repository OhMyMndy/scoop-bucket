{
    "version": "0.11.0.4609",
    "description": "云游戏, 让好玩触手可及",
    "homepage": "https://start.qq.com/",
    "license": "Unknown",
    "url": "https://imgcdn.start.qq.com/cdn/win.client/installer/START-installer-v0.11.0.4609.exe#/dl.7z",
    "hash": "md5:eefac7e5b529858e112bc0015a29d4cf",
    "installer": {
        "script": [
            "Expand-7zipArchive \"$dir\\app.7z\" \"$dir\"",
            "Remove-Item -Force -Recurse \"$dir\\`$PLUGINSDIR\"",
            "Remove-Item -Force -Recurse \"$dir\\app.7z\"",
            "Remove-Item -Force -Recurse \"$dir\\uninst*\"",
            "Remove-Item -Force -Recurse \"$dir\\logo*\""
        ]
    },
    "shortcuts": [
        [
            "START.exe",
            "START"
        ]
    ],
    "checkver": {
        "url": "https://api.start.qq.com/cfg/get?biztypes=windows-update-info-0",
        "regex": "latestversion.*?([\\d.]+)"
    },
    "autoupdate": {
        "url": "https://imgcdn.start.qq.com/cdn/win.client/installer/START-installer-v$version.exe#/dl.7z",
        "hash": {
            "url": "https://api.start.qq.com/cfg/get?biztypes=windows-update-info-0",
            "regex": "hashcode.*?$md5"
        }
    },
    "notes": "We don't persist your STSRT data, they are still storaged in '%APPDATA%\\Tencent\\START'."
}
